{
  "name": "analyse-tool",
  "version": "1.0.0",
  "private": true,
  "description": "A tool to analyse your webpack build result. It allows to browse the compilation and points out options to optimize the build.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint app scripts test lib",
    "beautify-lint": "beautify-lint lib/**.js app/**.js test/**.js",
    "test": "npm run lint && npm run beautify-lint && ./node_modules/.bin/karma start --browsers PhantomJS --single-run",
    "travis": "npm test",
    "beautify": "beautify-rewrite lib/**.js app/**.js test/**.js",
    "build": "npm run lint && npm run beautify-lint && ./node_modules/.bin/webpack",
    "dev-server": "node server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/webpack/analyse-tool.git"
  },
  "keywords": [
    "webpack",
    "analyse"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webpack/analyse-tool/issues"
  },
  "homepage": "https://github.com/webpack/analyse-tool#readme",
  "dependencies": {
    "babel-core": "^6.1.21",
    "babel-loader": "^6.1.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "css-loader": "^0.23.0",
    "file-loader": "^0.8.4",
    "normalize.css": "^3.0.3",
    "react": "^0.14.2",
    "react-addons-css-transition-group": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-toolbox": "^0.11.5",
    "redux": "^3.0.4",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.6"
  },
  "devDependencies": {
    "async": "^1.5.0",
    "autoprefixer": "^6.1.1",
    "babel-eslint": "^4.1.5",
    "babel-loader": "^6.2.0",
    "beautify-lint": "^1.0.4",
    "diff": "^2.2.1",
    "eslint": "^1.9.0",
    "eslint-plugin-babel": "^2.1.1",
    "eslint-plugin-nodeca": "^1.0.3",
    "eslint-plugin-react": "^3.8.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "glob": "^6.0.1",
    "html-webpack-plugin": "^1.7.0",
    "js-beautify": "^1.5.10",
    "karma": "^0.13.15",
    "karma-chrome-launcher": "^0.2.1",
    "karma-firefox-launcher": "^0.1.7",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-tap": "^1.0.3",
    "karma-tape-reporter": "^1.0.3",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.3",
    "node-sass": "^3.4.2",
    "phantomjs": "^1.9.19",
    "postcss-loader": "^0.8.0",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.1.2",
    "source-map-loader": "^0.1.5",
    "tape": "^4.2.2",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-dev-server": "^1.12.1",
    "webpack-hot-middleware": "^2.5.1"
  }
}
